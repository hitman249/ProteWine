{
  "name": "protewine",
  "version": "1.0.0",
  "main": "cache/server/server.js",
  "repository": "https://github.com/hitman249/protewine",
  "author": "hitman249",
  "description": "Portable Windows application with Wine technology.",
  "license": "MIT",
  "scripts": {
    "server": "yarn",
    "server-start": "electron .",
    "server-build": "yarn webpack --mode production --config webpack.config.js",
    "client": "cd client && yarn",
    "client-build": "cd client && yarn build",
    "client-watch": "cd client && yarn dev",
    "electron-builder": "electron-builder"
  },
  "build": {
    "appId": "protewine",
    "asar": true,
    "files": [
      "cache/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null,
    "linux": {
      "executableArgs": [
        "--no-sandbox"
      ],
      "target": "AppImage",
      "icon": "build/icons/512.png",
      "category": "Game",
      "desktop": {
        "X-AppImage-Integrate": "false",
        "X-AppImage-Name": "ProteWine",
        "X-AppImage-Arch": "x86_64",
        "X-AppImage-Version": "1.0.0"
      }
    }
  },
  "dependencies": {
    "cookie": "^0.6.0",
    "fetch-blob": "^4.0.0",
    "form-data": "^4.0.0",
    "glob": "^10.3.12",
    "helpful-decorators": "^2.1.0",
    "iconv-lite": "^0.6.3",
    "locutus": "^2.0.32",
    "lodash": "^4.17.21",
    "lzma": "^2.3.2",
    "md5-file": "^5.0.0",
    "node-fetch": "^3.3.2",
    "simple-vdf": "^1.1.1",
    "steamgriddb": "^2.2.0",
    "tree-kill": "^1.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@types/cookie": "^0.6.0",
    "@types/electron": "^1.6.10",
    "@types/locutus": "^0.0.8",
    "@types/lodash": "^4.17.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "electron": "^30.0.6",
    "electron-builder": "24.13.3",
    "eslint": "^9.3.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "yarn@4.2.1"
}
